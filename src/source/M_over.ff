module M_over
implicit none
private
contains
function subtract_char(string1,string2) result(diff)
character(len=*),intent(in) :: string1
character(len=*),intent(in) :: string2
   if(len(string1).ne.len(string2)then
      stop '*subtract_char* internal error: string lengths differ'
   endif
      ! if string is very long would cause overflow
      ! ignores case
      diff=base(string1,36)-base(string2,36)
integer :: diff
end function subtract_char
end module M_over
